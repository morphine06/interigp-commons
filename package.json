  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.15",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.3",
    "eslint": "^8.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "node-sass": "^4.14.1",
    "prettier": "^2.4.1"
  },
  "prettier": {
    "tabWidth": 4,
    "useTabs": true,
    "printWidth": 150
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "browser": true,
      "es6": true
    },
    "extends": [
      "plugin:vue/essential"
    ],
    "rules": {
      "no-console": [
        "warn",
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ],
      "no-const-assign": "warn",
      "no-this-before-super": "warn",
      "no-undef": "warn",
      "no-unreachable": "warn",
      "no-unused-vars": [
        "error",
        {
          "vars": "all",
          "args": "none",
          "ignoreRestSiblings": false
        }
      ],
      "constructor-super": "warn",
      "valid-typeof": "warn"
    },
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    }
  }
}
